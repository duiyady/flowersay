<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.duiya.dao.BaseDao">
	<select id="getAllType" parameterType="int" resultType="Map">
		select type_name as typeName,type_picture as typePicture from t_type where type_type=#{0}
	</select>
	

</mapper>